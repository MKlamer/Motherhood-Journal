            </attDef>
          </attList>
        </elementSpec>
        <elementSpec ident="bibl" module="core" mode="change">
          <attList>
            <attDef ident="ref" mode="replace" usage="req">
              <valList type="closed">
                <valItem ident="#Southey-ToADyingInfant"/>
                <valItem ident="#Saussure-LEducationProgressive"/>
