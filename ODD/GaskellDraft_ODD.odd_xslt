<span class="element">&lt;/quote&gt;</span>
<span class="element">&lt;/epigraph&gt;</span>
<span class="element">&lt;div<span class="attribute">type</span>="<span class="attributevalue">dedication</span>"&gt;</span>
<span class="element">&lt;p&gt;</span>For Ezra Pound <span class="element">&lt;q<span class="attribute">xml:lang</span>="<span class="attributevalue">it</span>"&gt;</span>il miglior fabbro.<span class="element">&lt;/q&gt;</span>
